<h1>index page</h1>

<% @task_lists.each do |task_list| %>
  <div class="clearfix">
    <h2 class="list-title"><%= link_to task_list.title, user_task_list_path(@user, task_list) %></h2>
    <p class="list-sub-title"><%= task_list.description %></p>
  </div>
<% end %>

<div class="links">
  <%= link_to "New Task List", new_user_task_list_path %>
</div>
